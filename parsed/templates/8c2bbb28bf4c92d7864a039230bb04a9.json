{"components/common/subscription-form":"<!--\n<h5 style=\"color: #948982;\">Be a Downlite VIP</h5>\n<h5 style=\"color: #221f1f;\">Sign up to get first dibs on great deals!</h5>\n{{#if settings.show_newsletter_summary }}\n    <div class=\"footer-newsletter-summary\"><p>{{settings.newsletter_summary}}</p></div>\n{{/if}}\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-mail\" /></svg>\n                </i>\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n        </div>\n    </fieldset>\n</form>\n-->\n\n<h5 style=\"color: #948982;\">Be a Downlite VIP</h5>\n<h5 style=\"color: #221f1f;\">Sign up to get first dibs on great deals!</h5>\n{{#if settings.show_newsletter_summary }}\n    <div class=\"footer-newsletter-summary\"><p>{{settings.newsletter_summary}}</p></div>\n{{/if}}\n\n<!--SPRINGBOT-->\n<div class=\"embeded-springot-form\">\n<div class=\"embeded-springot-form-form\"> \n<table border=\"0\" style=\"table-layout:fixed;border-collapse:collapse;border-width:0;margin-top:0;margin-bottom:0;margin-right:0;margin-left:0;\" cellpadding=\"0\" cellspacing=\"0\" align=\"center\" width=\"100%\" ng-click=\"$ctrl.select($event)\">\n    <tbody> \n        <tr style=\"display:table-row;vertical-align:middle;border-width:0;border-color:transparent;border:0;background:transparent;\"> \n            <td style=\"border-width:0;background:transparent;background-color:#FFFFFF;color:#000000;box-shadow:none;font-family:\\'Helvetica Neue\\',Helvetica,Arial,sans-serif;font-size:16px;font-weight:300;font-style:normal;text-transform:none;text-decoration:none;height:auto;line-height:1.5;text-align:left;opacity:1;text-shadow:none;padding:0;\" bgcolor=\"#FFFFFF\"> \n                <table class=\"columns\" border=\"0\" cellpadding=\"10\" cellspacing=\"0\" align=\"center\" width=\"100%\" style=\"table-layout:fixed;border-collapse:collapse;border-width:0;margin-top:0;margin-bottom:0;margin-right:0;margin-left:0;\"> \n                    <tbody> \n                        <tr style=\"display:table-row;vertical-align:middle;border-width:0;border-color:transparent;border:0;background:transparent;\"> \n                            <td drop-container=\"\" drop-accepts=\"$ctrl.disallowColumnsAndContainers($dragData)\" on-drop=\"$ctrl.onDrop($dragData)\" on-drag-enter=\"$ctrl.onDragEnter($event)\" on-drag-over=\"$ctrl.onDragOver($event, $dragData)\" on-drag-leave=\"$ctrl.onDragLeave($event)\" ng-repeat=\"col in $ctrl.cols track by $index\" align=\"center\" height=\"100%\" width=\"1%\" valign=\"top\" style=\"border-width:0;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;background:transparent;background-color:transparent;color:#000000;box-shadow:none;font-family:\\'Helvetica Neue\\',Helvetica,Arial,sans-serif;font-size:16px;font-weight:300;font-style:normal;text-transform:none;text-decoration:none;height:14px;line-height:1.5;text-align:left;opacity:1;text-shadow:none;\" ng-style=\"$ctrl.getColumnStyle($index)\" class=\"ng-scope drop-container responsive-column\"> \n\n                                <table style=\"table-layout:fixed;border-collapse:collapse;border-width:0;margin-top:0;margin-bottom:0;margin-right:0;margin-left:0;width:100%;background-color:#ffffff;color:#000000;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;\" ng-click=\"$ctrl.select($event)\">\n                                    <tbody> \n                                        <tr style=\"display:table-row;vertical-align:middle;border-width:0;border-color:transparent;border:0;background:transparent;\"> \n                                            <td bgcolor=\"#ffffff\" style=\"border-width:0;padding-top:0px;padding-right:0px;padding-bottom:0px;padding-left:0px;background:transparent;background-color:transparent;color:#000000;box-shadow:none;font-family:\\'Helvetica Neue\\',Helvetica,Arial,sans-serif;font-size:16px;font-weight:300;font-style:normal;text-transform:none;text-decoration:none;height:auto;line-height:1.5;text-align:left;opacity:1;text-shadow:none;\"> \n\n                                                <form class=\"springmod-form ng-pristine ng-valid\"> \n                                                    <!-- ngRepeat: input in $ctrl.inputs -->\n                                                    <div class=\"form-group ng-scope\" ng-repeat=\"input in $ctrl.inputs\" style=\"\"> \n                                                        <!-- ngIf: !$ctrl.usePlaceholder --> \n                                                        <!-- ngIf: !$ctrl.usePlaceholder --> \n                                                        <!-- ngIf: $ctrl.usePlaceholder -->\n                                                        <label for=\"springmod_email\" class=\"is-srOnly\">Enter Email</label>\n                                                        <input ng-if=\"$ctrl.usePlaceholder\" id=\"springmod_email\" type=\"email\" name=\"Email\" style=\"width:100%;margin:0;border-width:1px;border-style:solid;border-color:#cccccc;border-radius:1px;font-family:\\'Helvetica Neue\\',Helvetica,Arial,sans-serif;font-size:16px;height:32px;line-height:1.5;padding:0px 9px 0px 9px;box-sizing:border-box;background-color:#ffffff;color:#000000;\" placeholder=\"Email\" ondrop=\"event.preventDefault ? event.preventDefault() : event.returnValue = false\" ng-required=\"$ctrl.nameRequired || input.type === \\'email\\'\" class=\"ng-scope\" required=\"required\">\n                                                        <!-- end ngIf: $ctrl.usePlaceholder --> \n                                                    </div> \n                                                    <!-- end ngRepeat: input in $ctrl.inputs --> \n\n                                                    <div class=\"form-group\">\n                                                        <table style=\"table-layout:fixed;border-collapse:collapse;border-width:0;margin-top:0;margin-bottom:0;margin-right:0;margin-left:0;width:100%;background-color:#ffffff;color:#000000;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;\">\n                                                            <tbody> \n                                                                <tr style=\"display:table-row;vertical-align:middle;border-width:0;border-color:transparent;border:0;background:transparent;\"> \n                                                                    <td style=\"border-width:0;padding-top:0;padding-right:0;padding-bottom:0;padding-left:0;background:transparent;background-color:#ffffff;color:#000000;box-shadow:none;font-family:\\'Helvetica Neue\\',Helvetica,Arial,sans-serif;font-size:16px;font-weight:300;font-style:normal;text-transform:none;text-decoration:none;height:auto;line-height:1.5;opacity:1;text-shadow:none;\"> \n                                                                        <!-- ngIf: $ctrl.googleSiteKey && $ctrl.useCaptcha && $ctrl.canUseCaptcha --> \n                                                                        <input id=\"springmod_submit\" class=\"springmod_submit button button-primary\" value=\"Submit\" type=\"submit\"> \n                                                                    </td> \n                                                                </tr> \n                                                                <!-- ngIf: $ctrl.showPrivacy && $ctrl.canUsePrivacyLink -->\n                                                            </tbody> \n                                                        </table> \n                                                    </div> \n                                                    <!-- ngIf: $ctrl.showRestriction && $ctrl.canUseDoNotProcessDetails --> \n\n                                                    <div class=\"springmod-spinner\" id=\"springmod_spinner\"></div> \n                                                </form> \n                                            </td> \n                                        </tr>\n                                    </tbody> \n                                </table> \n                            </td> \n                            <!-- end ngRepeat: col in $ctrl.cols track by $index --> \n                        </tr>\n                    </tbody> \n                </table> \n            </td> \n        </tr>\n    </tbody> \n</table> \n</div>\n</div>\n\n"}
