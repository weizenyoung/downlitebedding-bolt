{"components/common/footer":"{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n{{/if}}\n\n{{#if settings.show_newsletter_box}}\n    <div class=\"footer-newsletter\">\n        {{> components/common/subscription-form}}\n    </div>\n{{/if}}\n                    \n<footer class=\"footer\" role=\"contentinfo\">\n    <div class=\"container\">\n        <section class=\"sixty-thirty\">\n            <div>\n                <h5 class=\"footer-info-heading\">Easy Returns</h5>\n                <p style=\"color: #000; font-size: 12px;\">You can return your order with all original materials included within 30 days of delivery. We will email you a prepaid FedEx label, and deduct the cost of the return shipping from your refund amount ($5 per item flat rate). <a href=\"/shipping-returns-warranty-policies/\" style=\"color: #011e41;\"><strong>Details</strong></a></p>\n                <div style=\"text-align: center;\">\n                    <a href=\"https://www.bbb.org/us/oh/mason/profile/bedding/downlite-0292-11258/#sealclick\" target=\"_blank\" rel=\"nofollow\"><img src=\"https://seal-cincinnati.bbb.org/seals/blue-seal-200-42-bbb-11258.png\" style=\"border: 0;\" alt=\"Downlite BBB Business Review\" /></a>\n                </div>\n            </div>\n            <div class=\"hide-size-tablet\">\n                {{#if theme_settings.social_icons_footer}}\n                    <article data-section-type=\"footer-webPages\">\n                        <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\n                        {{> components/common/social-links}}\n                    </article>\n                {{/if}}\n            </div>\n        </section>\n\n        <section class=\"footer-info 4a616d6573\">\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\n                <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-brands\">\n                    <h5 class=\"footer-info-heading\">\n                        {{lang 'footer.brands'}}\n                        <i class=\"icon\" aria-hidden=\"true\">\n                            <svg><use xlink:href=\"#icon-downselector\" /></svg>\n                        </i>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        {{#each shop_by_brand}}\n                            <li>\n                                <a href=\"{{url}}\">{{name}}</a>\n                            </li>\n                        {{/each}}\n                        <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                    </ul>\n                </article>\n            {{/and}}\n            <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-categories\">\n                <h5 class=\"footer-info-heading\">\n                    {{lang 'footer.categories'}}\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-downselector\" /></svg>\n                    </i>\n                </h5>\n                <ul class=\"footer-info-list\">\n                    {{#each categories}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    {{#if settings.gift_certificates_enabled}}\n                        <li>\n                            <a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n                        </li>\n                    {{/if}}\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col-multi footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <div class=\"footer-info-dropdown\">\n                    <h5 class=\"footer-info-heading\">\n                        Resources\n                        <i class=\"icon\" aria-hidden=\"true\">\n                            <svg><use xlink:href=\"#icon-downselector\" /></svg>\n                        </i>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        <li><a href=\"/blog/\">Sleep Blog</a></li>\n                        <li><a href=\"/bedding-buying-guide/\">Bedding Guide</a></li>\n                        <li><a href=\"/wedding-registry/\">Wedding Registry</a></li>\n                        <li><a href=\"/rds-responsibly-sourced-down/\">RDS - Responsibly Sourced Down</a></li>\n                        <li><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a></li>\n                    </ul>\n                </div>\n                <div class=\"footer-info-dropdown\">\n                    <h5 class=\"footer-info-heading\">\n                        About Us\n                        <i class=\"icon\" aria-hidden=\"true\">\n                            <svg><use xlink:href=\"#icon-downselector\" /></svg>\n                        </i>\n                    </h5>\n                    <ul class=\"footer-info-list\">\n                        <li><a href=\"/about-downlite/\">About Downlite</a></li>\n                        <li><a href=\"/one-percent-for-the-planet/\">One Percent For The Planet</a></li>\n                    </ul>\n                </div>\n            </article>\n            \n            <article class=\"footer-info-col footer-info-dropdown footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">\n                    Support\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-downselector\" /></svg>\n                    </i>\n                </h5>\n                <ul class=\"footer-info-list\">\n                    <li><a href=\"/accessibility/\">Accessibility</a></li>\n                    <li><a href=\"/shipping-returns-warranty-policies/\">Shipping & Returns</a></li>\n                    <li><a href=\"/contact-downlite/\">Contact Downlite</a></li>\n                    <li><a href=\"/faq/\">FAQ's</a></li>\n                    <li><a href=\"/warranty-policy/\">Warranty Policy</a></li>\n                    <li><a href=\"/privacy-policy/\">Privacy Policy</a></li>\n                </ul>\n            </article>\n        </section>\n        {{#if theme_settings.geotrust_ssl_common_name}}\n            <div class=\"footer-geotrust-ssl-seal\">\n                {{> components/common/geotrust-ssl-seal}}\n            </div>\n        {{/if}}\n        \n        <div class=\"hide-size-desktop\">\n            {{#if theme_settings.social_icons_footer}}\n                <article data-section-type=\"footer-webPages\" style=\"margin-bottom: 20px;\">\n                    <h5 style=\"text-align: center; font-weight: 400; text-transform: none; margin-bottom: 0;\">{{lang 'social.connect'}}</h5>\n                    {{> components/common/social-links}}\n                </article>\n            {{/if}}\n        </div>\n        \n        <div class=\"footer-copyright\">\n            {{> components/common/payment-icons}}\n            {{#if theme_settings.show_copyright_footer}}\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n            {{/if}}\n            <p class=\"powered-by\">*Shipping to Contiguous US Only (Sorry Alaska &amp; Hawaii)</p>\n        </div>\n        \n        {{#or theme_settings.show_powered_by theme_settings.show_built_by}}\n            <div class=\"footer-copyright\" style=\"font-size: 12px;\">\n                {{#if theme_settings.show_powered_by}}\n                    <p class=\"powered-by\">Powered by <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\n                {{/if}}\n                {{#if theme_settings.show_built_by}}\n                    <p class=\"powered-by\">Built by <a href=\"https://www.weizenyoung.com/\" rel=\"nofollow\">Weizen Young</a></p>\n                {{/if}}\n            </div>\n        {{/or}}\n    </div>\n</footer>\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_applepay}}\n        <svg class=\"footer-payment-icon footer-payment-icon-apple\"><use xlink:href=\"#icon-logo-apple-pay\"></use></svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<!--\n<h5 style=\"color: #948982;\">Be a Downlite VIP</h5>\n<h5 style=\"color: #221f1f;\">Sign up to get first dibs on great deals!</h5>\n{{#if settings.show_newsletter_summary }}\n    <div class=\"footer-newsletter-summary\"><p>{{settings.newsletter_summary}}</p></div>\n{{/i